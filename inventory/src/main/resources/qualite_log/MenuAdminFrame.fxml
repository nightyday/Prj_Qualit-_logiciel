<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="menuVBox" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="qualite_log.MenuAdminFrame">
  <children>
    <MenuBar fx:id="menuAdminMenuBar" VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Matériel">
          <items>
            <MenuItem fx:id="toolListMenuItem" mnemonicParsing="false" text="Consulter les matériels" />
            <MenuItem fx:id="toolCreateMenuItem" mnemonicParsing="false" text="Créer un matériel" />
            <MenuItem fx:id="toolUpdateMenuItem" mnemonicParsing="false" text="Modifier un matériel" />
            <MenuItem fx:id="toolDeleteMenuItem" mnemonicParsing="false" text="Supprimer un matériel" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Type Matériel">
          <items>
            <MenuItem fx:id="toolTypeListMenuItem" mnemonicParsing="false" text="Consulter les types de matériel" />
            <MenuItem fx:id="toolTypeCreateMenuItem" mnemonicParsing="false" text="Créer un type de matériel" />
            <MenuItem fx:id="toolTypeDeleteMenuItem" mnemonicParsing="false" text="Supprimer un type de matériel" />
          </items>
        </Menu>
            <Menu mnemonicParsing="false" text="Utilisateurs">
               <items>
                  <MenuItem fx:id="userListMenuItem" mnemonicParsing="false" text="Consulter les utilisateurs" />
                  <MenuItem fx:id="userCreateMenuItem" mnemonicParsing="false" text="Créer un utilisateur" />
                  <MenuItem fx:id="userUpdateMenuItem" mnemonicParsing="false" text="Modifier un utilisateur" />
                  <MenuItem fx:id="userDeleteMenuItem" mnemonicParsing="false" text="Supprimer un utilisateur" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Déconnexion">
               <items>
                  <MenuItem fx:id="deconnexionMenuItem" mnemonicParsing="false" text="Se déconnecter" />
               </items>
            </Menu>
      </menus>
    </MenuBar>
  </children>
</VBox>
